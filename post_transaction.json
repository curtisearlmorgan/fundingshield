    GuardianCertificateRequest:
      type: object
      required:
        - clientId
        - clientUniqueId
        - acctNumberLastFive
        - abaNumber
      properties:
        clientId:
          description: Unique Identifier for Client of Guaridan Client
          type: string
        clientUniqueId:
          description: Guardian Client Unique Identifier typically Loan Number
          type: string
        acctNumberLastFive:
          description: Last 5 digits of the Wire Account Number
          type: string
        abaNumber:
          description: Wire Account ABA/Routing Number
          type: string
          pattern: '^\d{9}$'
          example: 012345678
        #closingAgentName:
        #  description: name of the Closing Agent
        #  type: string
    GuardianCertificateResponse:
      allOf:
        - $ref: '#/components/schemas/GuardianCertificateRequest' # GuardianResponse has all properties of a GuardianCertificateRequest
        - properties:
            certificateId:
              type: string
              format: uuid
              example: d290f1ee-6c54-4b01-90e6-d701748f0851           
            nameOnClosingAgentWireAccount:
              type: string
              example: Title Agent of America Escrow Account
              description: Account Name on the Closing Agent Wire Account
            intermediateBankName:
              type: string
              example: First Bank of Oklahoma
              description: Closing Agent Intermediate Wire Account Bank Name
            intermediateBankAccountNumber:
              type: string
              example: "12345"
              description: Closing Agent Intermediate Wire Account Number
            result:
              type: string
              enum: [Data not found, Data not matching, Data matching]
