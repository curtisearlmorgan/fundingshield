servers:
  - url: 'https://guardian-api.fundingshield.com/guardian'
components:
  securitySchemes:
    apiKeyHeader:
      description: |-
        the subscription_key assocaited with your username.  Contact
        ```
        sales@fundingshield.com

        ```
        to get a subscription_key. 
      type: apiKey
      name: Ocp-Apim-Subscription-Key
      in: header
    apiKeyQuery:
      description: |-
        the subscription_key assocaited with your username.  Contact
        ```
        sales@fundingshield.com

        ```
        to get a subscription_key. 
      type: apiKey
      name: subscription-key
      in: query
  schemas:
    # States:
    #   type: string
    #   example: NY
      # enum: [AK, AL, AR, AS, AZ, CA, CO, CT, DC, DE, FL, GA, GU, HI, IA, ID, IL, IN, KS, KY, LA, MA, MD, ME, MI, MN, MO, MP, MS, MT, NC, ND, NE, NH, NJ, NM, NV, NY, OH, OK, OR, PA, PR, RI, SC, SD, TN, TX, UM, UT, VA, VI, VT, WA, WI, WV, WY]     
    BorrowerInfo:
      type: object
      required:
        - primaryBorrowerFirstName
        - primaryBorrowerLastName
      properties:
        primaryBorrowerFirstName:
          type: string
          example: John
        primaryBorrowerMiddleName:
          type: string
          example: Joe
        primaryBorrowerLastName:
          type: string
          example: Smith
        primaryBorrowerIsNonBorrowingParty:
          type: boolean
          default: false
        secondaryBorrowerFirstName:
          type: string
          example: John
        secondaryBorrowerMiddleName:
          type: string
          example: Joe
        secondaryBorrowerLastName:
          type: string
          example: Smith
        secondaryBorrowerIsNonBorrowingParty:
          type: boolean
          default: false          
        tertiaryBorrowerFirstName:
          type: string
          example: John
        tertiaryBorrowerMiddleName:
          type: string
          example: Joe
        tertiaryBorrowerLastName:
          type: string
          example: Smith
        tertiaryBorrowerIsNonBorrowingParty:
          type: boolean
          default: false                
        quarternaryBorrowerFirstName:
          type: string
          example: John
        quarternaryBorrowerMiddleName:
          type: string
          example: Joe
        quarternaryBorrowerLastName:
          type: string
          example: Smith
        quarternaryBorrowerIsNonBorrowingParty:
          type: boolean
          default: false             
    PropertyInfo:
      type: object
      required:
        - propertyAddress1
        - propertyCity
        - propertyState
        - propertyZipCode        
      properties:
        propertyAddress1:
          type: string
          example: 123 Main St.
        propertyAddress2:
          type: string
          example: Suite 1500
        propertyCity:
          type: string
          example: New York
        propertyState:
          # $ref: '#/components/schemas/States'          
          type: string
          example: NY
          pattern: '/^[a-z]{2}$/i'
        propertyZipCode:
          type: string
          example: 10001 or 10001-0001
          pattern: '^[0-9]{5}(?:-[0-9]{4})?$'
    ClosingAgentInfo:
      type: object
      required:
        - closingAgentName
        - closingAgentAddress1
        - closingAgentCity
        - closingAgentState
        - closingAgentZipCode
      properties:
        clientClosingAgentId:
          type: integer
        closingAgentName:
          type: string
          example: Title Agent of America
        closingAgentAddress1:
          type: string
          example: 123 Main St.
        closingAgentAddress2:
          type: string
          example: Suite 1500
        closingAgentCity:
          type: string
          example: New York
        closingAgentState:
          # $ref: '#/components/schemas/States'          
          type: string
          example: NY
          pattern: '/^[a-z]{2}$/i'
        closingAgentZipCode:
          type: string
          example: 10001 or 10001-0001
          pattern: '^[0-9]{5}(?:-[0-9]{4})?$'           
        closingAgentEmail:
          type: string
          example: closings@example.com
          format: email
        closingAgentPhone:
          type: string
          example: 800-555-1234
          pattern: '^\(?([0-9]{3})\)?[-.●]?([0-9]{3})[-.●]?([0-9]{4})$'
        closingAgentContactFirstName:
          type: string
          example: John
        closingAgentContactLastName:
          type: string
          example: Smith          
    WireAccountInfo:
      type: object
      required:
        - abaNumber
        - acctNumber
        - nameOnClosingAgentWireAccount
      properties:
        abaNumber:
          description: Wire Account ABA/Routing Number
          type: string
          pattern: '^\d{9}$'
          example: 012345678
        acctNumber:
          type: string
          example: 12345
        nameOnClosingAgentWireAccount:
          type: string
          example: Title Agent of America Escrow Account
        intermediateBankName:
          type: string
          example: First Bank of Oklahoma
        intermediateBankAccountNumber:
          type: string
          example: 12345          
        bankName:
          type: string
          example: Chase
        bankAddress1:
          type: string
        bankAddress2:
          type: string
        bankCity:
          type: string
        bankState: 
          # $ref: '#/components/schemas/States'          
          type: string
          example: NY
          pattern: '/^[a-z]{2}$/i' 
        bankZipCode:
          type: string
          example: 10001 or 10001-0001
          pattern: '^[0-9]{5}(?:-[0-9]{4})?$' 
        bankPhone:
          type: string
          example: 800-555-1234
          pattern: '^\(?([0-9]{3})\)?[-.●]?([0-9]{3})[-.●]?([0-9]{4})$'
    UnderwriterInfo:
      type: object
      properties:
        titleInsuranceUnderwriterName:
          type: string
          example: ABC Underwriter
    LoanInfo:
      type: object
      required:
        - clientUniqueId
        - loanAmount
      properties:
        clientUniqueId:
          type: string
          example: 18-123456789
        clientUniqueId2:
          type: string
          example: 18-123456789
        loanAmount:
          type: string
          example: 453100
        closingDate:
          type: string
          example: 2017-07-21T17:32:28Z
          format: date-time
        secondMortgage:
          type: string
          example: true
        secondMortgageInsured:
          type: string
          example: true
        secondMortgageClientUniqueID:
          type: string
          example: 18-123456789
        secondMortgageLoanAmount:
          type: string
          example: 453100
        titleNumber:
          type: string
          example: 123456
        escrowNumber:
          type: string
          example: 123456
        rushOrder:
          type: boolean
          example: true
          default: false
    LenderInfo: 
      required:
        - lenderLegalName
        - lenderUserEmail
      properties:
        lenderLegalName:
          type: string   
        lenderUserFirstName:
          type: string
        lenderUserLastName:
          type: string
        lenderUserPhone:
          type: string
          example: 800-555-1234
          pattern: '^\(?([0-9]{3})\)?[-.●]?([0-9]{3})[-.●]?([0-9]{4})$'          
