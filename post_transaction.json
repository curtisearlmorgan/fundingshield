{
openapi: 3.0.0
info:
  version: "2.0"
  title: FundingShield Guardian API
  description: This is the FundingShield Guardian API
security:
  - apiKeyHeader: []
  - apiKeyQuery: []
paths:
  /transaction:
    get:
      tags:
        - guardian-external
      summary: searches Guardian transactions
      operationId: searchGuardianTransactions
      description: |
        Get Guardian transaction details by passing a transactionId
      parameters:
        - in: query
          name: transactionId
          description: Guardian transactionId
          required: true
          schema:
            type: integer
      responses:
        '200':
          description: No error, operation successful.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/GuardianResponse'
        '400':
          description: bad input parameter
    post:
      tags:
        - guardian-external
      summary: Creates a single Guardian Transaction
      operationId: createGuardianTransaction
      description: POST creates a single Guardian Transaction
      responses:
        '201':
          description: Successful creation of a resource.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/GuardianResponse'
        '400':
          description: bad input parameter
      requestBody:
        content:
          multipart/form-data:
            schema:
              $ref: '#/components/schemas/GuardianTransaction'
            encoding:
              pdfClosingProtectionLetter:
                contentType: image/pdf
              pdfWireInstructions:
                contentType: image/pdf
              pdfErrorsAndOmissionsInsurance:
                contentType: image/pdf
              pdfCrimesPolicy:
                contentType: image/pdf
              pdfFidelityBond:
                contentType: image/pdf
              pdfBankReferenceLetter:
                contentType: image/pdf   
              pdfStateLicense:   
                contentType: image/pdf   
              pdfLenderTitleCommitment:
                contentType: image/pdf
        description: Guardian Transaction(s) to add
    patch:
      tags:
        - guardian-external
      summary: updates a single Guardian Transaction
      operationId: updateGuardianTransaction
      description: updates a single Guardian Transaction
      responses:
        '200':
          description: No error, operation successful.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/GuardianResponse'
        '400':
          description: bad input parameter
      parameters:
        - in: query
          name: transactionId
          description: Guardian transactionId
          required: true
          schema:
            type: integer          
      requestBody:
        content:
          multipart/form-data:
            schema:
              $ref: '#/components/schemas/GuardianTransactionPatch'
            encoding:
              pdfClosingProtectionLetter:
                contentType: image/pdf
              pdfWireInstructions:
                contentType: image/pdf
              pdfErrorsAndOmissionsInsurance:
                contentType: image/pdf
              pdfCrimesPolicy:
                contentType: image/pdf
              pdfFidelityBond:
                contentType: image/pdf
              pdfBankReferenceLetter:
                contentType: image/pdf   
              pdfStateLicense:   
                contentType: image/pdf   
              pdfLenderTitleCommitment:
                contentType: image/pdf
        description: Guardian Transaction to add        
  /certificate:
    post:
      tags:
        - guardian-external
      summary: searches Guardian Certificates
      operationId: searchGuardianCertificates
      description: |
        By passing in the appropriate options, you can search for
        available Guardian Transactions in the system
      responses:
        '200':
          description: No error, operation successful.
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/GuardianCertificateResponse'
        '400':
          description: bad input parameter
      requestBody:
        content:
          application/json:
            schema:
              type: array
              items:
                $ref: '#/components/schemas/GuardianCertificateRequest'             
        description: Paremeters for Guardian Certificate search
  /transactionDocs:
    get:
      tags:
        - guardian-external    
      summary: Get Guardian Certified Documents
      parameters:
        - in: query
          name: transactionId
          description: unique identifier for Guardian Transaction obtained from GET /transaction
          required: true
          schema:
            type: integer
        - in: query
          name: type
          description: the type of requested Guardian Certified Document
          required: true
          schema:
            type: string
            enum: [pdfGuardianCertificate, zipAllGuardianCertifiedDocs]
      responses:
        '200':
          description: Guardian Certified Documents
          content:
            application/octet-stream:
              schema:
                type: string
                format: binary    
    post:
      tags:
        - guardian-external    
      summary: Upload Guardian Certified Documents
      parameters:
        - in: query
          name: transactionId
          description: unique identifier for Guardian Transaction obtained from GET /transaction
          required: true
          schema:
            type: integer
      requestBody:
        content:
          multipart/form-data:
            schema:
              $ref: '#/components/schemas/TransactionDocuments'
            encoding:
              pdfClosingProtectionLetter:
                contentType: image/pdf
              pdfWireInstructions:
                contentType: image/pdf
              pdfErrorsAndOmissionsInsurance:
                contentType: image/pdf
              pdfCrimesPolicy:
                contentType: image/pdf
              pdfFidelityBond:
                contentType: image/pdf
              pdfBankReferenceLetter:
                contentType: image/pdf   
              pdfStateLicense:   
                contentType: image/pdf   
              pdfLenderTitleCommitment:
                contentType: image/pdf         
      responses:
        '201':
          description: Successful creation of a resource.                   
